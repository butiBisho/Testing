<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body ng-app="uiroute">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Travel Start", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a ui-sref="Index">Flight</a></li>
                    <li><a ui-sref="CarIndex">Car</a></li>
                    <li><a ui-sref="HotelIndex">Hotel</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript" src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/ui/1.12.0/jquery-ui.js"></script>
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Plugin/angular/angular.min.js"></script>
    <script src="~/Plugin/angular-ui-router/release/angular-ui-router.min.js"></script>
    <script src="~/App/App.module.js"></script>
    <script src="~/App/App.config.js"></script>

    @*Flight*@

    <script src="~/App/RegisterController.js"></script>
    <script src="~/App/LoginController.js"></script>

    <script src="~/App/IndexController.js"></script>
    <script src="~/App/SearchController.js"></script>
    <script src="~/App/FilterSearchController.js"></script>
    <script src="~/App/TravellerController.js"></script>
    <script src="~/App/ExtrasController.js"></script>
    <script src="~/App/Driver.js"></script>
    <script src="~/App/Return.js"></script>
    <script src="~/App/ClientReturn.js"></script>

    <script src="~/App/FlightPayment.js"></script>
    @*Flight*@

    @*hotel*@
    <script src="~/App/HotelRegister.js"></script>
    <script src="~/App/HotelLogin.js"></script>

    <script src="~/App/HotelIndexController.js"></script>
    <script src="~/App/HotelSearch.js"></script>
    <script src="~/App/HotelFilterSearch.js"></script>
    <script src="~/App/SelectRoom.js"></script>

    <script src="~/App/HotelPayment.js"></script>

    <script src="~/App/UserHotelFilterSearch.js"></script>
    <script src="~/App/UserSelectRoom.js"></script>

    <script src="~/App/fupController.js"></script>
    @*hotel*@

    @*Car*@
    <script src="~/App/CarRegister.js"></script>
    <script src="~/App/CarLogin.js"></script>

    <script src="~/App/CarIndexController.js"></script>
    <script src="~/App/PartnerController.js"></script>
    <script src="~/App/CarSearch.js"></script>
    <script src="~/App/FilterCarController.js"></script>

    <script src="~/App/CarPayment.js"></script>
    @*Car*@

    @*Admin*@
    <script src="~/App/AdminRegisterController.js"></script>
    <script src="~/App/AdminLoginController.js"></script>

    <script src="~/App/AdminFlightController.js"></script>
    <script src="~/App/AdminCarController.js"></script>
    <script src="~/App/AdminHotelController.js"></script>
    <script src="~/App/Bisho.js"></script>

    <script src="~/App/AdminDashboard.js"></script>
    <script src="~/App/Additional.js"></script>
    <script src="~/App/ClientController.js"></script>
    @*Admin*@

    @*Client*@
    <script src="~/App/Dashboard.js"></script>
    <script src="~/App/Booking.js"></script>
    @*Client*@

    @RenderSection("scripts", required: false)
</body>
</html>
